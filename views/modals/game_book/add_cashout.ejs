<div class="modal fade" id="modal-add-cashout" tabindex="-1" role="dialog" aria-labelledby="modal-default-popout"
	aria-hidden="true" data-keyboard="false" data-bs-backdrop="static">
	<div class="modal-dialog modal-dialog-centered modal-md" role="document">
		<div class="modal-content">
			<div class="block block-rounded block-themed block-transparent mb-0">
				<div class="block-header bg-primary-dark">
					<h3 class="block-title">Add CHIPS RETURN</h3>
					<div class="block-options">
						<button type="button" class="btn-block-option" data-bs-dismiss="modal" aria-label="Close">
							<i class="fa fa-fw fa-times"></i>
						</button>
					</div>
				</div>
				<form id="add_cashout">
					<input type="hidden" name="game_id" class="game_list_id">
					<div class="block-content">

						<!-- <div class="row mb-2">
							<div class="col-sm-12">
								<input type="number" class="form-control text-center txtAmount" placeholder="Amount" name="txtAmount">
							</div>
						</div> -->

						<div class="row mb-4">
							<div class="col-sm-6">
								<input type="text" onkeypress="return onlyNumberKey(event)" data-type="number" class="form-control text-center txtNN" placeholder="NN Chips" name="txtNN">
							</div>

							<div class="col-sm-6">
								<input type="text" onkeypress="return onlyNumberKey(event)" data-type="number" class="form-control text-center txtCC" placeholder="CC Chips" name="txtCC">
							</div>
						</div>

						<div class="row mb-2">
							<div class="col-sm-4">
								<div class="form-check form-block">
									<input class="form-check-input" type="radio" value="1" id="cash1" name="txtTransType">
									<label class="form-check-label" for="cash1">
										<span class="d-flex align-items-center">
											Cash
										</span>
									</label>
								</div>
							</div>
							<div class="col-sm-4">
								<div class="form-check form-block">
									<input class="form-check-input" type="radio" value="2" id="deposit1" name="txtTransType">
									<label class="form-check-label" for="deposit1">
										<span class="d-flex align-items-center">
											Deposit
										</span>
									</label>
								</div>
							</div>
							<div class="col-sm-4">
								<div class="form-check form-block">
									<input class="form-check-input" type="radio" value="3" id="marker1" name="txtTransType">
									<label class="form-check-label" for="marker1">
										<span class="d-flex align-items-center">
											Marker
										</span>
									</label>
								</div>
							</div>
						</div>

						

					</div>
					<div class="block-content block-content-full text-end bg-body">
						<button type="submit" class="btn btn-sm btn-primary" data-bs-dismiss="modal">
							Save
						</button>
						<button type="button" class="btn btn-sm btn-alt-secondary" data-bs-dismiss="modal">
							Close
						</button>
					</div>
				</form>
			</div>
		</div>
	</div>
</div>