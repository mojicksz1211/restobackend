<div class="modal fade" id="modal-edit-account" role="dialog" aria-labelledby="modal-default-popout" aria-hidden="true"
	data-keyboard="false" data-bs-backdrop="static">
	<div class="modal-dialog modal-dialog-centered" role="document">
		<div class="modal-content">
			<div class="block block-rounded block-themed block-transparent mb-0">
				<div class="block-header bg-primary-dark">
					<h3 class="block-title">Edit Account</h3>
					<div class="block-options">
						<button type="button" class="btn-block-option" data-bs-dismiss="modal" aria-label="Close">
							<i class="fa fa-fw fa-times"></i>
						</button>
					</div>
				</div>
				<form id="edit_account">
					<div class="block-content">
						<div class="row mb-2">
							<div class="col-sm-12">
								<input type="text" class="form-control" name="txtAgentName" id="edit_agent_name"
									placeholder="Agent Name" disabled>
							</div>
						</div>
						<div class="row mb-2">
							<div class="col-sm-12">
								<input type="text" class="form-control" name="txtAgencyName" id="edit_agency_name" placeholder="Agency"
									disabled>
							</div>
						</div>
						<div class="row mb-2">
							<div class="col-sm-12">
								<input type="text" class="form-control" name="txtGuestNo" placeholder="Guest No" id="guest_no">
							</div>
						</div>
						<div class="row mb-2">
							<div class="col-sm-12">
								<input type="text" class="form-control" name="txtMembershipNo" placeholder="Membership No"
									id="membership_no">
							</div>
						</div>
					</div>
					<div class="block-content block-content-full text-end bg-body">
						<button type="submit" class="btn btn-sm btn-primary" data-bs-dismiss="modal">Save</button>
						<button type="button" class="btn btn-sm btn-alt-secondary" data-bs-dismiss="modal">Close</button>
					</div>
				</form>
			</div>
		</div>
	</div>
</div>