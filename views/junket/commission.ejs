<%- include ('../partial/header') %>

    <!-- Main Container -->
    <main id="main-container">
        <div class="content">
            <!-- Dynamic Table Responsive -->
            <div class="block block-rounded">
                <div class="block-header block-header-default">
                    <h3 class="block-title">
                        <small>Junket / Commission</small>
                    </h3>
                    <nav class="flex-shrink-0 my-2 my-sm-0 ms-sm-3" aria-label="breadcrumb">
                        <ol class="breadcrumb">
                            <!-- <button type="button" class="btn btn-primary" onclick="addCapital()">
                                <i class="fa fa-plus" aria-hidden="true"></i> New Record
                            </button> -->
                        </ol>
                    </nav>
                </div>
                <div class="block-content block-content-full">
                    <div class="row mb-2">
                        <div class="col-sm-12">
                            <table id="#dataTable" class="table table-bordered table-vcenter">
                                <thead>
                                  <tr>
                                    <th class="text-center" style="width: 150px;">TOTAL ROLLING</th>
                                    <th class="text-center" style="width: 150px;">DUE TO AGENT</th>
                                    <th class="text-center" style="width: 150px;">EXPENSE</th>    
                                    <th class="text-center" style="width: 150px;">ACTUAL TO AGENT</th>    
                                  </tr>
                                </thead>
                                <tbody>
                                  <tr>
                                    <td class="fw-bold text-end"><span class="total_rolling"></span></td>
                                    <td class="fw-bold text-end"><span class="total_due"></span></td>
                                    <td class="fw-bold text-end"><span class="total_expense"></span></td>             
                                    <td class="fw-bold text-end"><span class="total_actual"></span></td>             
                                  </tr>       
                                </tbody>
                            </table>
                        </div>                                            
                    </div> 

                    <!-- DataTables init on table by adding .js-dataTable-responsive class, functionality is initialized in js/pages/be_tables_datatables.min.js which was auto compiled from _js/pages/be_tables_datatables.js -->
                    <table id="commission-tbl" class="table table-bordered table-vcenter js-dataTable-responsive">
                        <thead>
                            <tr>
                                <th class="d-none d-sm-table-cell" style="width: 150px;">GAME REF</th>
                                <th class="d-none d-sm-table-cell">ACCOUNT CODE</th>
                                <th class="d-none d-sm-table-cell">ACCOUNT NAME</th>
                                <th class="d-none d-sm-table-cell">TOTAL ROLLING</th>
                                <th class="d-none d-sm-table-cell">DUE TO AGENT</th>
                                <th class="d-none d-sm-table-cell">EXPENSE</th>
                                <th class="d-none d-sm-table-cell">NET TO AGENT</th>
                                <th class="d-none d-sm-table-cell">ACTUAL TO AGENT</th>
                                <th class="d-none d-sm-table-cell">REMARKS</th>
                                <th class="d-none d-sm-table-cell">CASHIER</th>
                                <th class="d-none d-sm-table-cell">MANAGER</th>
                                <th class="text-center" style="width: 50px;">ACTION</th>
                            </tr>
                        </thead>
                        <tbody>
                        </tbody>
                    </table>
                </div>
            </div>
            <!-- Dynamic Table Responsive -->
        </div>
        <!-- END Page Content -->
    </main>
    <!-- END Main Container -->

