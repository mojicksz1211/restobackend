{
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES2020",
    "checkJs": false,
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "*": ["*", "node_modules/*"]
    },
    "skipLibCheck": true
  },
  "exclude": ["node_modules"],
  "typeAcquisition": {
    "enable": false
  }
}

